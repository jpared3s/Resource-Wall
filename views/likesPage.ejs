<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Home Page</title>

    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <link rel="stylesheet" href="/vendor/normalize-4.1.1.css" />
    <link rel="stylesheet" href="/vendor/border-box.css" />
    <link rel="stylesheet" href="/styles/main.css" />
    <link rel="stylesheet" href="/styles/layout.css" />
    <link rel="stylesheet" href="/styles/likes.css" />
    <script src="../routes/likes.js"></script>
    <script src="/vendor/jquery-3.0.0.js"></script>
    <script defer src="/scripts/app.js"></script>
  </head>
  <body>
    <h1 class="page-name">Likes Page</h1>

    <form class="article-container" method="get" action="/likes">
      <% for (let resource in resources) { %>
      <article>
        <a class="resource-title" href="<%=resources[resource]["title"]%>"><%= resources[resource]["title"]%></a>
        <p><%= resources[resource]["description"] %></p>
        <p><%= resources[resource]["tags"] %></p>
        <p><%= resources[resource]["user_id"]%></p>
      </article>
      <% } %>
    </form>
  </body>
</html>
